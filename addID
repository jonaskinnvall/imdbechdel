
function addID(){

var Bech_data = d3.csv("Bechdel_dataset/bechdel_allmovies.csv", function(data) {
  						data.forEach(function(d) {
  						d.imdbid = +d.imdbid;
  						d.id = +d.id;
  						d.year = +d.year;
  						d.rating = +d.rating;
  						});
  					console.log(data[0)
					});

var IMDB_data = d3.csv("IMDB_movies/imdb_movie_metadata.csv", function(data) {
						data.forEach(function(d)){
							d.num_critic_for_reviews = +d.num_critic_for_reviews;
							d.duration = +d.duration;
							d.director_facebook_likes = +d.director_facebook_likes;
							d.actor_3_facebook_likes = +d.actor_3_facebook_likes;
							d.actor_1_facebook_likes = +d.actor_1_facebook_likes;
							d.gross = +d.gross;

						});


					});



}